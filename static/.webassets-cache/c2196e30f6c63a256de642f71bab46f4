var DragDrop,
  __bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

DragDrop = (function() {

  function DragDrop() {
    this.dragListener = __bind(this.dragListener, this);
    this.dragoverListener = __bind(this.dragoverListener, this);
    this.dropHandlers = __bind(this.dropHandlers, this);
  }

  DragDrop.prototype.initialize = function() {
    return this.dropHandlers();
  };

  DragDrop.prototype.dropHandlers = function() {
    var drop;
    drop = $('#drop_area');
    this.dragoverListener();
    return this.dropListener();
  };

  DragDrop.prototype.dragoverListener = function() {
    var _this = this;
    return $.on('dragover', function(event) {
      return console.log(event);
    });
  };

  DragDrop.prototype.dragListener = function() {
    var _this = this;
    return $.on('drag', function(event) {
      return console.log(event);
    });
  };

  return DragDrop;

})();

window.DragDrop = new DragDrop;
