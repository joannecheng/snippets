var DragDrop,
  __bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

DragDrop = (function() {

  function DragDrop() {
    this.dropHandlers = __bind(this.dropHandlers, this);    console.log('dragging');
    this.dropHandlers();
  }

  DragDrop.prototype.dropHandlers = function() {
    var _this = this;
    console.log('drop:window');
    $(document).bind('drop dragover', function(e) {
      return e.preventDefault();
    });
    return $('#fileupload').fileupload({
      dataType: 'json',
      dropZone: $(document),
      done: function(e, data) {
        return $('body').append($('<p/>').text(file.name));
      }
    });
  };

  return DragDrop;

})();

window.DragDrop = new DragDrop;
